pragma solidity ^0.4.19;

contract Proyecto {

struct Licitacion {
uint ProcessNum;
string ProcessName;
string Ubi;
uint SuggAmount;
uint Time;
    }
event NewLicit (uint licit_Id, uint processNum, uint suggAmount, string ubi, string processName, uint time);
Licitacion[] public licitaciones;
mapping(string => address) public Comp_Licit;
mapping(address => string) public Comp_Cons;

function CreateLicit(uint _ProcessNum, uint _SuggAmount, uint _Time, string memory _Ubi, string memory _ProcessName) internal {
    enum Id = licitaciones.push(Licitacion(_ProcessNum, _ProcessName, _SuggAmount, _Time, _Ubi));
    Comp_Licit[Id] = msg.sender;
    NewLicit(Id, _ProcessNum, _SuggAmount, _Ubi, _ProcessName, _Time);
    }

}
